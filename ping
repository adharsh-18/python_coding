import requests
import os
os.system("ls")
hostname = "google.com"
response = os.system("ping -c 1 " + hostname)

if response == 0:
  print ("hostname 'is up!'")
else:
  print ("hostname, 'is down!'")

x = requests.get('https://w3schools.com')
status_code = x.status_code
if status_code == 200:
  print ("hostname, 'is up!'")
else:
  print ("hostname, 'is down!'")
